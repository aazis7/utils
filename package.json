{
  "name": "@aazis7/utils",
  "version": "0.2.4",
  "homepage": "https://github.com/aazis7/utils.git",
  "license": "MIT",
  "author": "aazis7",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./string": {
      "types": "./dist/string/index.d.ts",
      "require": "./dist/string/index.js",
      "import": "./dist/string/index.mjs"
    },
    "./number": {
      "types": "./dist/number/index.d.ts",
      "require": "./dist/number/index.js",
      "import": "./dist/number/index.mjs"
    },
    "./object": {
      "types": "./dist/object/index.d.ts",
      "require": "./dist/object/index.js",
      "import": "./dist/object/index.mjs"
    },
    "./time": {
      "types": "./dist/time/index.d.ts",
      "require": "./dist/time/index.js",
      "import": "./dist/time/index.mjs"
    },
    "./validate": {
      "types": "./dist/validate/index.d.ts",
      "require": "./dist/validate/index.js",
      "import": "./dist/validate/index.mjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist && echo 'Success...'",
    "dev": "tsup --watch",
    "format": "prettier -w .",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "prepublishOnly": "npm run test && npm run type-check && npm run build",
    "test": "vitest",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/node": "^24.2.1",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.3.0",
    "jiti": "^2.5.1",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.2.0",
    "prettier-plugin-packagejson": "^2.5.19",
    "prettier-plugin-sort-json": "^4.1.1",
    "terser": "^5.43.1",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.39.1",
    "vitest": "^3.2.4"
  },
  "packageManager": "npm@10.9.3",
  "engines": {
    "node": ">=22"
  }
}
